{
    "name": "wwwtemplate",
    "version": "1.0.0",
    "description": "Стартовый проект для сборки и разработки веб-макетов и UI Kit к ним.",
    "scripts": {
        "dev": "parcel src/index.html --open",
        "kit": "parcel src/uikit.html --open",
        "shop": "parcel src/shop.html --open",
        "signup": "parcel src/signup.html --open",
        "signin": "parcel src/signin.html --open",
        "product": "parcel src/product.html --open",
        "build:uikit": "rm -rf build/uikit && parcel build src/uikit.html --dist-dir build/uikit",
        "build": "rm -rf build/uikit && parcel build src/index.html --dist-dir build/www"
    },
    "license": "ISC",
    "dependencies": {
        "autoprefixer": "^10.4.2",
        "parcel": "^2.2.1",
        "postcss-modules": "^4.3.0",
        "sass": "^1.26.5"
    },
    "browserslist": [
        "last 5 version",
        "ie 10"
    ],
    "devDependencies": {
        "@parcel/transformer-sass": "^2.2.1"
    }
}
